{
  "name": "@reapit/storybook-addon-html",
  "version": "5.0.1",
  "description": "A Storybook addon that extracts and displays compiled syntax-highlighted HTML",
  "main": "out/preset.js",
  "files": [
    "out/**/*",
    "README.md",
    "*.js"
  ],
  "keywords": [
    "addon",
    "storybook",
    "html",
    "storybook-addon",
    "code",
    "popular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whitespace-se/storybook-addon-html.git"
  },
  "scripts": {
    "build": "yarn clean && babel -d ./out ./src",
    "build:watch": "babel --watch -d ./out ./src",
    "clean": "rm -rf ./out",
    "prepare": "yarn build",
    "prepublishOnly": "cp ../README.md ."
  },
  "author": "Whitespace",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "babel-eslint": "^10.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.1.21",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.13.9",
    "@babel/parser": "^7.13.12",
    "@storybook/api": "^6.1.21",
    "@storybook/components": "^6.1.21",
    "prettier": "~2.2.1",
    "react-syntax-highlighter": "^15.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "icon": "https://avatars1.githubusercontent.com/u/10450088?s=400&u=b2a95469b7d2addf63366a93a17f0bd06f9894cc&v=4",
    "displayName": "HTML preview"
  }
}
